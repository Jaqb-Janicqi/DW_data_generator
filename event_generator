import numpy as np
import random


Types=["delay","breakdown","accident"]

#types of events
for i in range(10):
    Type = random.choice(Types)
    print(Type)
    

for i in range(10):
    DelayAmount = np.random.choice([0,100])
    print(DelayAmount)










